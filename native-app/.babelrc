{
    "presets": [
        "babel-preset-expo",
        "react-native"
    ],
    "env": {
    "development": {
        "plugins": ["transform-react-jsx-source"]
    }
    },
    "plugins": [
        ["module-alias", [
            { "src": "./assets", "expose": "assets" },
            { "src": "./src/store", "expose": "store" }
        ]]
    ]
}
